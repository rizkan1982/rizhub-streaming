{
  "version": 2,
  "name": "rizhub-backend",
  "builds": [
    {
      "src": "build/src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "build/src/index.js"
    }
  ],
  "functions": {
    "build/src/index.js": {
      "maxDuration": 60
    }
  }
}

